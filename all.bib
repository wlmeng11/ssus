	month = oct,
	year = {2016},
	keywords = {Array signal processing, field programmable gate arrays, Field programmable gate arrays, Ultrasonic imaging, 256-channel open scanner, 256-channel ultrasound advanced open platform, Digital signal processing, High-frame-rate (HFR) imaging, Iron, logic design, multiple digital signal processors, open platform, Probes, programmable ultrasound system, Real-time systems, research scanner, signal processing, ultrasound methods},
	pages = {1488--1495},
