	year = {1999},
	keywords = {biomedical transducers, biomedical ultrasonics, medical image processing, Ultrasonic imaging, ultrasonic transducers, Focusing, Ultrasonic transducers, Phased arrays, Signal processing, digital signal processing, Digital signal processing chips, Control systems, Matlab, ultrasonic focusing, Real time systems, 24 GByte, 40 MHz, 5 to 10 s, advanced imaging methods, dedicated chips, dynamic receive focusing, experimental ultrasound system, Image storage, medical diagnostic imaging, Performance evaluation, phased array B-mode imaging, real-time synthetic imaging, synthetic aperture imaging},
	pages = {1595--1599 vol.2},
