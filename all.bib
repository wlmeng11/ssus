	file = {Full Text PDF:/home/kelu/Zotero/storage/4PG7A6CN/Wall - 2010 - A High-Speed Reconfigurable System for Ultrasound .pdf:application/pdf;Snapshot:/home/kelu/Zotero/storage/NJ5H8SG4/Wall - 2010 - A High-Speed Reconfigurable System for Ultrasound .html:text/html}
}

